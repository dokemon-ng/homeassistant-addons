{
  "name": "Dokémon Agent Test",
  "version": "latest",
  "slug": "dokemon_agent_test",
  "description": "Connects to a Dokémon server",
  "url": "https://github.com/dokemon-ng/",
  "arch": ["amd64", "armv7", "aarch64"],
  "image": "javastraat/dokemon-agent",
  "startup": "application",
  "boot": "auto",
  "init": false,
  "map": [
    "addon_config:rw",
    "homeassistant_config:rw",
    "share:rw",
    "media:rw"
  ],
    "options": {
      "SERVER_URL": ""
    },
    "schema": {
      "SERVER_URL": "str?"
    },
    "hassio_api": false,
    "homeassistant_api": false,
    "host_network": true,
    "hassio_role": "manager",
    "privileged": [
      "SYS_ADMIN",
      "SYS_RAWIO"
    ],
    "maps": [
      "/var/run/docker.sock:/var/run/docker.sock:ro",
    "addon_config:rw",
    "homeassistant_config:rw",
    "share:rw",
    "config:rw"
    ],    
    "docker_api": true,
    "full_access": true


}
