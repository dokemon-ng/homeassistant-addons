{
  "name": "Dokémon Agent Test",
  "version": "latest",
  "slug": "dokemon_agent_test",
  "description": "Connects to a Dokémon server",
  "url": "https://github.com/dokemon-ng/",
  "arch": ["amd64", "armv7", "aarch64"],
  "image": "javastraat/dokemon-agent",
  "startup": "application",
  "boot": "auto",
  "host_network": true,
  "ports": {
    "9090/tcp": 9090
  },
  "options": {
    "enabled": true,
    "SERVER_URL": "http://your-dokemon-server-ip:9090",
    "TOKEN": "your-agent-token-here",
  },
  "schema": {
    "enabled": "bool",
    "SERVER_URL": "str",
    "TOKEN": "str",
  },
  "environment": {
    "TZ": "Europe/Amsterdam"
  },
  "arch": ["armv7", "amd64", "arm64"],
  "boot": "auto"
}
