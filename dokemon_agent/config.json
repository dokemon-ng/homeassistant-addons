{
  "name": "Dokemon Agent",
  "version": "latest",
  "slug": "dokemon_agent",
  "description": "Connects to a Dokemon server",
  "arch": ["amd64", "armv7", "aarch64"],
  "image": "javastraat/dokemon-agent",
  "startup": "application",
  "boot": "auto",
  "init": false,
  "ports": {},
  "volumes": {
    "docker_sock": "/var/run/docker.sock"
  },
  "options": {
    "server_url": "http://your-server-ip:9090",
    "token": "your-token-here"
  },
  "schema": {
    "server_url": "str",
    "token": "str"
  },
  "env": {
    "SERVER_URL": {"name": "SERVER_URL", "value": "{{ options.server_url }}"},
    "TOKEN": {"name": "TOKEN", "value": "{{ options.token }}"}
  }
}
