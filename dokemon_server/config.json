{
  "name": "Dokémon Server",
  "version": "latest",
  "slug": "dokemon_server",
  "description": "Dokémon is a friendly GUI for managing Docker Containers.",
  "url": "https://github.com/dokemon-ng/",
  "arch": ["amd64", "armv7", "aarch64"],
  "image": "javastraat/dokemon-server",
  "startup": "application",
  "boot": "auto",
  "init": false,
  "ports": {"9090/tcp": 9090},
  "volumes": {"data": "/data", "docker_sock": "/var/run/docker.sock"},
  "options": {},
  "schema": {},
  "host_network": true,
  "homeassistant_api": true,
  "hassio_api": true,
  "stdin": true,
  "docker_api": true,
  "hassio_role": "manager",
  "host_dbus": true
}
